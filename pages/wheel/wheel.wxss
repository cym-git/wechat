/* pages/wheel/wheel.wxss */
._title {
  width: 100%;
}
.wheel {
  width: 654rpx;
  height: 654rpx;
  margin: 0 auto;
  transform: translateY(-125rpx);
}
.wheel-bg {
  width: 100%;
  height: 100%;
}
.header {
  text-align: center;
}
.header .wheel-titile {
  font-size: 32rpx;
}
.header .goback {
  width: 20rpx;
  height: 38rpx;
  display: inline-block;
  position: relative;
  left: -280rpx;
  top: 10rpx;
  background-image: url('data:image/png;base64,');
  background-size: cover; 
}
.main .wheel {
  position: relative;
}
.main .wheel .begin {
  width: 140rpx;
  height: 178rpx;
  background-image: url('data:image/png;base64,'); 
  background-size: cover;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%); 
  z-index: 11;
}
/* canvas转盘 */
.canvas-container .canvas-content {
  width: 654rpx;
  height: 654rpx;
  position: relative;
  text-align: center;
}
/* 外圆 */
.yuan {
  position: absolute;
  overflow: hidden; 
  border-radius: 50%;
  z-index: 9;
  width: 100%;
  height: 100%;
}
/* 线条 -- 父盒子 */
.canvas-line{
  position: absolute;
  left: 0;
  top: 0;
  width: inherit;
  height: inherit;
  z-index: 9;
  background-color: #FFD943;
}
/* 线条 -- 子盒子 */
.canvas-litem{
  position: absolute;
  left: 327rpx;
  top: 0;
  width: 1px;
  height: 327rpx;
  background-color: white;
  -webkit-transform-origin: 50% 327rpx; 
  transform-origin: 50% 327rpx;
}
/* 线条文字 */
.canvas-list{
  position: absolute;
  left: 0;
  top: 0;
  width: inherit;
  height: inherit;
  z-index: 9;
}
/* 文字 -- 子盒子 */
.canvas-item{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  color: #801414;
  font-weight: bold;
  text-shadow: 0 1px 1px rgba(255,255,255,.6);
}
/* 文字 -- 子盒子 */
.canvas-item-text{
  position: relative;
  display: block;
  padding-top: 100rpx;
  margin: 0 auto;
  text-align: center; 
  -webkit-transform-origin: 50% 327rpx;  
  transform-origin: 50% 327rpx; 
  z-index: 1;
}
/* 圆框 */
.kuang {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9;
  width: 100.4%;
  height: 100.4%;
}
/* 转盘阴影 */
.wheel-shadow {
  width: 470rpx;
  height: 80rpx;
  transform: translateY(-10rpx);
}
/* 转盘样式结束, 游戏介绍开始 */
.game-intro {
  text-align: center;
  transform:translateY(-50rpx);
}
.game-intro .prizeNum {
  color: white;
  font-size: 30rpx;
  line-height: 80rpx;
}
.prizeNum>text {
  color: #FCFF00;
}
/* 介绍 */
.intro {
  line-height: 40rpx;
  font-size: 24rpx;
  width: 690rpx;
  /* height: 300rpx; */
  margin: 17rpx auto;
  background-color: #E9553E;
  border-radius: 10rpx;
  padding: 20rpx 33rpx;
  box-sizing: border-box;
}
._intro:first-child {
  margin-bottom: 27rpx;
}
.intro .title {
  float: left;
}
.intro .content {
  text-align: left;
  margin-left: 120rpx;
}
