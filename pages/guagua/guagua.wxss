/* pages/guagua/guagua.wxss */
/* 可以复用的样式 */
._title {
  width: 100%;
}
.header .goback {
  width: 20rpx;
  height: 38rpx;
  display: inline-block;
  position: relative;
  left: -280rpx;
  top: 10rpx;
  background-image: url('data:image/png;base64,');
  background-size: cover; 
}
/* 转盘样式结束, 游戏介绍开始 */
.game-intro {
  text-align: center;
  /* transform:translateY(-50rpx); */
}
.game-intro .prizeNum {
  color: white;
  font-size: 30rpx;
  line-height: 80rpx;
}
.prizeNum>text {
  color: #FCFF00;
}
.footer {
  padding-bottom: 20rpx;
}
/* 介绍 */
.intro {
  line-height: 40rpx;
  font-size: 24rpx;
  width: 690rpx;
  /* height: 300rpx; */
  margin: 17rpx auto;
  background-color: #E9553E;
  border-radius: 10rpx;
  padding: 20rpx 33rpx;
  box-sizing: border-box;
}
._intro:first-child {
  margin-bottom: 27rpx;
}
.intro .title {
  float: left;
}
.intro .content {
  text-align: left;
  margin-left: 120rpx;
}
/* 刮刮乐核心样式 */
.gua-main {
  width: 100%;
  position: relative;
}
.content-text {
  background-size: cover;
  position: absolute;
  width: 650rpx;
  height: 300rpx;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  text-align: center;
  line-height: 300rpx;
  background-image: url('data:image/png;base64,');
}
.gua {
  width: 650rpx;
  height: 300rpx;
  margin: 0 auto;
}